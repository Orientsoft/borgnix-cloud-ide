{
  "name": "ace",
  "version": "0.0.0",
  "authors": [
    "Eddie Huang <inoc603@gmail.com>"
  ],
  "moduleType": [
    "node"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "install": {
    "path": "public/vendor",
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/dist/css/bootstrap.min.css",
        "bower_components/bootstrap/dist/js/bootstrap.js"
      ],
      "ace-builds": [
        "bower_components/ace-builds/src-min-noconflict/*.js"
      ],
      "react-bootstrap-treeview": [
        "bower_components/react-bootstrap-treeview/src/react-bootstrap-treeview.jsx",
        "bower_components/react-bootstrap-treeview/src/react-bootstrap-treeview.css"
      ]
    }
  },
  "dependencies": {
    "bootstrap": "~3.3.5",
    "react": "~0.13.3",
    "requirejs": "~2.1.18",
    "ace-builds": "~1.1.9",
    "react-bootstrap-treeview": "~0.1.0",
    "react-bootstrap": "~0.23.5",
    "pubsub-js": "~1.5.2",
    "dot-object": "~0.11.0",
    "bootstrap-contextmenu": "~0.3.3"
  }
}
